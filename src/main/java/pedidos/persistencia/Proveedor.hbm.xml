<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="src.appLIFIA.src.Usuario" table="usuarios"> 
		<id name="id" column="id">
			<generator class="native"/>
		<property name="nombre"/>
		<property name="cuil"/>
		<property name="direccion"/>
		<property name="coordenadaX"/>
		<property name="coordenadaY"/>
		<list name="productos" cascade="save-update">
		    <key column="idProveedor" foreign-key="FK_PROV_PROD"/>
		    <many-to-many class="src.appLIFIA.src.Usuario"/>
		</list>
	</class>
</hibernate-mapping>
